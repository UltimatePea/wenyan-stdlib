/*Delta 技術審查測試*//*Author: Delta, PR Critic*//*加載陣列函數*//*取陣列長度函數*/var 取陣列長度=_=>{};取陣列長度=陣列=>{return 陣列;};/*取陣列元素函數*/var 取陣列元素=_=>{};取陣列元素=陣列=>索引=>{if (索引<1){console.log();return 0;};if (索引>陣列.length){console.log();return 0;};return undefined;};/*陣列追加函數*/var 陣列追加=_=>{};陣列追加=陣列=>新元素=>{陣列.push(新元素);return 陣列;};/*陣列求和函數*/var 陣列求和=_=>{};陣列求和=陣列=>{var 總和=0;var 索引=0;總和=0;索引=1;while(true){if (索引>陣列.length){break;};const _ans1=總和+undefined;總和=_ans1;const _ans2=索引+1;索引=_ans2;};return 總和;};/*陣列搜尋函數*/var 陣列搜尋=_=>{};陣列搜尋=陣列=>目標值=>{var 索引=0;索引=1;while(true){if (索引>陣列.length){break;};if (undefined陣列[索引-1]==目標值){return 索引;};const _ans3=索引+1;索引=_ans3;};return 0;};/*=== 開始測試 ===*/console.log();/*測試1：基本陣列操作*/var 測試陣列=[];測試陣列.push(10);測試陣列.push(20);測試陣列.push(30);var 長度=0;const _ans4=取陣列長度(測試陣列);var 長度=_ans4;console.log();console.log();var 第一元素=0;const _ans5=取陣列元素(測試陣列);var 第一元素=_ans5;console.log();console.log();/*測試2：陣列追加*/const _ans6=陣列追加(測試陣列);var 測試陣列=_ans6;const _ans7=取陣列長度(測試陣列);var 長度=_ans7;console.log();console.log();/*測試3：陣列求和*/var 總和=0;const _ans8=陣列求和(測試陣列);var 總和=_ans8;console.log();console.log();/*測試4：陣列搜尋*/var 搜尋結果=0;const _ans9=陣列搜尋(測試陣列);var 搜尋結果=_ans9;console.log();console.log();console.log();